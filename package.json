    "@types/classnames": "^2.3.1",
    "@types/react-dom": "^17.0.9",
    "classnames": "^2.3.1",
    "next": "^11.1.2",
    "next-images": "^1.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sharp": "^0.29.1",
    "tailwindcss": "^2.2.16",
    "urql": "^2.0.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/react": "^17.0.27",
    "autoprefixer": "^10.3.7",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "postcss": "^8.3.9",
