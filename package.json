{
  "name": "keystone-app",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock .keystone .next",
    "postinstall": "keystone-next postinstall",
    "dev": "keystone-next dev",
    "start": "keystone-next start",
    "build": "keystone-next build",
